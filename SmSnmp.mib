  motherboard OBJECT-TYPE
    SYNTAX  Motherboard
    ACCESS  not-accessible
    STATUS  mandatory
  ::= { hitachiSM-SAGroupsDefinition 7 }

  Motherboard ::= SEQUENCE
  {
    numberOfExpansionSlots  INTEGER,
    fruGroupIndex           INTEGER,
    operationalGroupIndex   INTEGER
  }

  numberOfExpansionSlots OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The total number of expansion slots which physically exist on the
       motherboard whether occupied or not (see System Slots group)."
  ::= { motherboard 1 }

  fruGroupIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "If this is a Field Replaceable Unit or if it is part of another FRU, this
       provides an index into the FRU table; 'value = -1' indicates that the
       group is not a FRU."
  ::= { motherboard 2 }

  operationalGroupIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The index into the Operational State Table for this device if
       applicable."
  ::= { motherboard 3 }


