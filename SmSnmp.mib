  hitachi         OBJECT IDENTIFIER  ::= { enterprises   116 }

--------------------------------------------------------------------------------

  systemExMib         OBJECT IDENTIFIER  ::= { hitachi             5 }
  systemManagerMibs   OBJECT IDENTIFIER  ::= { systemExMib        21 }
  information         OBJECT IDENTIFIER  ::= { systemManagerMibs   1 }

--------------------------------------------------------------------------------
-- System Manager
-- information
--------------------------------------------------------------------------------

  hitachiSM-SAGroupsDefinition OBJECT IDENTIFIER  ::= { information     1 }

--------------------------------------------------------------------------------
  componentid OBJECT-TYPE
    SYNTAX  componentid
    ACCESS  not-accessible
    STATUS  mandatory
  ::= { hitachiSM-SAGroupsDefinition 1 }

  componentid ::= SEQUENCE
  {
    manufacturer  DisplayString,
    product       DisplayString,
    version       DisplayString,
    serialNumber  DisplayString,
    installation  DisplayString,
    verify        INTEGER
  }

  manufacturer OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..64))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Manufacturer of this system. It is expressed in the form of string
       (ASCII Code)."
  ::= { componentid 1 }

  product OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..64))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Product name for this system. It is expressed in the form of string
       (ASCII Code)."
  ::= { componentid 2 }

  version OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..64))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Version number of this system. It is expressed in the form of string
       (ASCII Code)."
  ::= { componentid 3 }

  serialNumber OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..64))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Serial number for this system. It is expressed in the form of string
       (ASCII Code)."
  ::= { componentid 4 }

  installation OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..25))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The time and date when this component was installed. It is expressed in
       the form of string(ASCII Code)."
  ::= { componentid 5 }

  verify OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "A code that provides a level of verification that the component is still
       installed and working. It is indicated by a integer value as follows 
       (hexadecimal).
         0x00 = An error occurred; check status code.
         0x01 = This component does not exist.
         0x02 = Verification is not supported.
         0x03 = Reserved.
         0x04 = This component exists, but the functionality is untested.
         0x05 = This component exists, but the functionality is unknown.
         0x06 = This component exists, and is not functioning correctly.
         0x07 = This component exists, and is functioning correctly."
  ::= { componentid 6 }


--------------------------------------------------------------------------------
  operatingSystem OBJECT IDENTIFIER ::= { hitachiSM-SAGroupsDefinition 3 }

  operatingSystemTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF OperatingSystem
    ACCESS  not-accessible
    STATUS  mandatory
  ::= { operatingSystem 1 }

  operatingSystemEntry OBJECT-TYPE
    SYNTAX  OperatingSystem
    ACCESS  not-accessible
    STATUS  mandatory
    INDEX   { operatingSystemIndex }
  ::= { operatingSystemTable 1 }

  OperatingSystem ::= SEQUENCE
  {
    operatingSystemIndex                  INTEGER,
    operatingSystemName                   DisplayString,
    operatingSystemVersion                DisplayString,
    primaryOperatingSystem                INTEGER,
    operatingSystemBootDeviceStorageType  INTEGER,
    operatingSystemBootDeviceIndex        INTEGER,
    operatingSystemBootPartitionIndex     INTEGER,
    operatingSystemDescription            DisplayString
  }

  operatingSystemIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The index into the operating system table."
  ::= { operatingSystemEntry 1 }

  operatingSystemName OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..64))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The name of this operating system. It is expressed in the form of string
       (ASCII Code)."
  ::= { operatingSystemEntry 2 }

  operatingSystemVersion OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..64))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The version number of this operating system. It is expressed in the form
       of string(ASCII Code)."
  ::= { operatingSystemEntry 3 }

  primaryOperatingSystem OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "If true, this is the primary operating system. It is indicated by a
       integer value as follows (hexadecimal).
         0x00 = False
         0x01 = True"
  ::= { operatingSystemEntry 4 }

  operatingSystemBootDeviceStorageType OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "An index into the Disks Table to indicate the device from which this
       operating system was booted. To fully access the Disks Table, this index
       must be combined with the attribute Boot Device Index. It is indicated by
       a integer value as follows (hexadecimal).
         0x01 = Other
         0x02 = Unknown
         0x03 = Hard Disk
         0x04 = Floppy Disk
         0x05 = Optical ROM
         0x06 = Optical WORM
         0x07 = Optical RW
         0x08 = Compact Disk
         0x09 = Flash Disk
         0x0A = Bernoulli
         0x0B = Optical Floppy Disk"
  ::= { operatingSystemEntry 5 }

  operatingSystemBootDeviceIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "An index into the Disks Table."
  ::= { operatingSystemEntry 6 }

  operatingSystemBootPartitionIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "An index into the Partition table indicating the partition from which
       this operating system booted."
  ::= { operatingSystemEntry 7 }

  operatingSystemDescription OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..128))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "A description of this operating system. It is expressed in the form of
       string(ASCII Code)."
  ::= { operatingSystemEntry 8 }


--------------------------------------------------------------------------------
  systemBios OBJECT IDENTIFIER ::= { hitachiSM-SAGroupsDefinition 4 }

  systemBiosTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF SystemBios
    ACCESS  not-accessible
    STATUS  mandatory
  ::= { systemBios 1 }

  systemBiosEntry OBJECT-TYPE
    SYNTAX  SystemBios
    ACCESS  not-accessible
    STATUS  mandatory
    INDEX   { biosIndex }
  ::= { systemBiosTable 1 }

  SystemBios ::= SEQUENCE
  {
    biosIndex             INTEGER,
    biosManufacturer      DisplayString,
    biosVersion           DisplayString,
    biosRomSize           INTEGER,
    biosStartingAddress   OCTET STRING,
    biosEndingAddress     OCTET STRING,
    biosLoaderVersion     DisplayString,
    biosReleaseDate       DisplayString,
    primaryBios           INTEGER
  }

  biosIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The index into the system BIOS table."
  ::= { systemBiosEntry 1 }

  biosManufacturer OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..64))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The name of the company that wrote this System BIOS. It is expressed in
       the form of string(ASCII Code)."
  ::= { systemBiosEntry 2 }

  biosVersion OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..64))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The version number or version string of this BIOS. It is expressed in the
       form of string(ASCII Code)."
  ::= { systemBiosEntry 3 }

  biosRomSize OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The physical size of this BIOS ROM device in kilobytes."
  ::= { systemBiosEntry 4 }

  biosStartingAddress OBJECT-TYPE
    SYNTAX  OCTET STRING (SIZE(1..8))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The starting physical address for the memory which the BIOS occupies. It
       is indicated by a 8bytes value laid out as big endian (byte 0 is the most
       significant, byte 7 is the least significant)."
  ::= { systemBiosEntry 5 }

  biosEndingAddress OBJECT-TYPE
    SYNTAX  OCTET STRING (SIZE(1..8))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The ending physical address for the memory which the BIOS occupies. It is
       indicated by a 8bytes value laid out as big endian (byte 0 is the most
       significant, byte 7 is the least significant)."
  ::= { systemBiosEntry 6 }

  biosLoaderVersion OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..64))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The BIOS flash loader version number or string. It is expressed in the
       form of string(ASCII Code)."
  ::= { systemBiosEntry 7 }

  biosReleaseDate OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..25))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The BIOS release date. It is expressed in the form of string
       (ASCII Code)."
  ::= { systemBiosEntry 8 }

  primaryBios OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "If true, this is the primary System BIOS. It is indicated by a integer
       value as follows (hexadecimal).
         0x00 = False
         0x01 = True"
  ::= { systemBiosEntry 9 }


