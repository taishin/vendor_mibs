  systemCache OBJECT IDENTIFIER ::= { hitachiSM-SAGroupsDefinition 10 }

  systemCacheTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF SystemCache
    ACCESS  not-accessible
    STATUS  mandatory
  ::= { systemCache 1 }

  systemCacheEntry OBJECT-TYPE
    SYNTAX  SystemCache
    ACCESS  not-accessible
    STATUS  mandatory
    INDEX   { systemCacheIndex }
  ::= { systemCacheTable 1 }

  SystemCache ::= SEQUENCE
  {
    systemCacheIndex            INTEGER,
    systemCacheLevel            INTEGER,
    systemCacheSpeed            INTEGER,
    systemCacheSize             INTEGER,
    systemCacheWritePolicy      INTEGER,
    systemCacheErrorCorrection  INTEGER,
    fruGroupIndex               INTEGER,
    operationalGroupIndex       INTEGER,
    systemCacheType             INTEGER
  }

  systemCacheIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "An index into the System Cache table."
  ::= { systemCacheEntry 1 }

  systemCacheLevel OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Is this the primary System Cache, or a subsidiary cache? It is indicated
       by a integer value as follows (hexadecimal).
         0x01 = Other
         0x02 = Unknown
         0x03 = Primary
         0x04 = Secondary
         0x05 = Tertiary"
  ::= { systemCacheEntry 2 }

  systemCacheSpeed OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The speed of this System Cache module in nanoseconds."
  ::= { systemCacheEntry 3 }

  systemCacheSize OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The size of this System Cache module in kilobytes."
  ::= { systemCacheEntry 4 }

  systemCacheWritePolicy OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Is this a write-back or a write-through cache? It is indicated by a
       integer value as follows (hexadecimal).
         0x01 = Other
         0x02 = Unknown
         0x03 = Write Back
         0x04 = Write Through"
  ::= { systemCacheEntry 5 }

  systemCacheErrorCorrection OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The error correction scheme supported by this cache component. It is
       indicated by a integer value as follows (hexadecimal).
         0x01 = Other
         0x02 = Unknown
         0x03 = None
         0x04 = Parity
         0x05 = Single Bit ECC
         0x06 = MultiBit ECC"
  ::= { systemCacheEntry 6 }

  fruGroupIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "If this is a Field Replaceable Unit or if it is part of another FRU, this
       provides an index into the FRU table; 'value = -1' indicates that the
       group is not a FRU."
  ::= { systemCacheEntry 7 }

  operationalGroupIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The index into the Operational State Table for this device."
  ::= { systemCacheEntry 8 }

  systemCacheType OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "This defines the system cache type. It is indicated by a integer value as
       follows (hexadecimal).
         0x01 = Other
         0x02 = Unknown
         0x03 = Instruction
         0x04 = Data
         0x05 = Unified"
  ::= { systemCacheEntry 9 }


--------------------------------------------------------------------------------
  powerSupply OBJECT IDENTIFIER ::= { hitachiSM-SAGroupsDefinition 17 }

  powerSupplyTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF PowerSupply
    ACCESS  not-accessible
    STATUS  mandatory
  ::= { powerSupply 1 }

  powerSupplyEntry OBJECT-TYPE
    SYNTAX  PowerSupply
    ACCESS  not-accessible
    STATUS  mandatory
    INDEX   { powerSupplyIndex }
  ::= { powerSupplyTable 1 }

  PowerSupply ::= SEQUENCE
  {
    powerSupplyIndex                    INTEGER,
    fruGroupIndex                       INTEGER,
    operationalGroupIndex               INTEGER,
    powerUnitIndex                      INTEGER,
    powerSupplyType                     INTEGER,
    inputVoltageCapabilityDescription   DisplayString,
    range1InputVoltageLow               INTEGER,
    range1InputVoltageHigh              INTEGER,
    range1VoltageProbeIndex             INTEGER,
    range1ElectricalCurrentProbeIndex   INTEGER,
    range2InputVoltageLow               INTEGER,
    range2InputVoltageHigh              INTEGER,
    range2VoltageProbeIndex             INTEGER,
    range2CurrentProbeIndex             INTEGER,
    activeInputVoltageRange             INTEGER,
    inputVoltageRangeSwitching          INTEGER,
    range1InputFrequencyLow             INTEGER,
    range1InputFrequencyHigh            INTEGER,
    range2InputFrequencyLow             INTEGER,
    range2InputFrequencyHigh            INTEGER,
    totalOutputPower                    INTEGER
  }

  powerSupplyIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The index number of the current power supply."
  ::= { powerSupplyEntry 1 }

  fruGroupIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "If this is a Field Replaceable Unit or if it is part of another FRU, this
       provides an index into the FRU table; 'value = -1' indicates that the
       group is not a FRU."
  ::= { powerSupplyEntry 2 }

  operationalGroupIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The index into the Operational State table for this device."
  ::= { powerSupplyEntry 3 }

  powerUnitIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "This is the ID of the Power Unit (logical power supply) to which this
       Power Supply belongs. This value matches up with the Power Unit Table
       Index attribute defined in the Power Unit Globe Table."
  ::= { powerSupplyEntry 4 }

  powerSupplyType OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "This attribute describes the type of Power Supply, e.g. Linear,
       Switching, Battery, etc. It is indicated by a integer value as follows 
       (hexadecimal).
         0x01 = Other
         0x02 = Unknown
         0x03 = Linear
         0x04 = Switching
         0x05 = Battery
         0x06 = UPS
         0x07 = Converter
         0x08 = Regulator"
  ::= { powerSupplyEntry 5 }

  inputVoltageCapabilityDescription OBJECT-TYPE
    SYNTAX  DisplayString (SIZE (1..80))
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "An expanded description of the Input Voltage Capability for this Power
       Supply. It is expressed in the form of string(ASCII Code)."
  ::= { powerSupplyEntry 6 }

  range1InputVoltageLow OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The low voltage of Input Voltage Range 1 for this Power Supply, in
       millivolts. A value of 0x80000000 denotes 'unknown'."
  ::= { powerSupplyEntry 7 }

  range1InputVoltageHigh OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The high voltage of Input Voltage Range 1 for this Power Supply, in
       millivolts. A value of 0x80000000 denotes 'unknown'."
  ::= { powerSupplyEntry 8 }

  range1VoltageProbeIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Index into the Voltage Probe table identifying the voltage probe
       monitoring this input range. 0 indicates no probe provided."
  ::= { powerSupplyEntry 9 }

  range1ElectricalCurrentProbeIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Index into the Elelctrical Current Probe table identifying the Electrical
       current probe monitoring this input range. 0 indicates no probe
       provided."
  ::= { powerSupplyEntry 10 }

  range2InputVoltageLow OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The low voltage of Input Voltage Range 2 for this Power Supply, in
       millivolts. A value of 0x80000000 denotes 'unknown'."
  ::= { powerSupplyEntry 11 }

  range2InputVoltageHigh OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The high voltage of Input Voltage Range 2 for this Power Supply, in
       millivolts. A value of 0x80000000 denotes 'unknown'."
  ::= { powerSupplyEntry 12 }

  range2VoltageProbeIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Index into the Voltage Probe table identifying the voltage probe
       monitoring this input range. 0 indicates no probe provided."
  ::= { powerSupplyEntry 13 }

  range2CurrentProbeIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "Index into the Current Probe table identifying the current probe
       monitoring this input range. 0 indicates no probe provided."
  ::= { powerSupplyEntry 14 }

  activeInputVoltageRange OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
      "The value of this attribute indicates which input voltage range of this
       Power Supply is currently active. It is indicated by a integer value as
       follows (hexadecimal).
         0x01 = Other
         0x02 = Unknown
         0x03 = Range 1
         0x04 = Range 2
